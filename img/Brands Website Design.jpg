                    ga('send', {
                        hitType: 'event',
                        eventCategory: eventCategory,
                        eventAction: action,
                        hitCallback: cbWithCounter
                    });
                } else {
                    cbCounter++;
                }
            }

            var sendAnalyticOrTimeout = decorateTimeout(sendData, 3000);

            $('.mac-download').click(function (e) {
                e.preventDefault();

                analyticsData.AppName = 'mac-app';
